    "Table": {
        "prefix": "bscf-table",
        "body": [
            "<table class=\"table\">",
            "    <thead>",
            "        <tr>",
            "            <th scope=\"col\">Color</th>",
            "            <th scope=\"col\">First</th>",
            "            <th scope=\"col\">Last</th>",
            "            <th scope=\"col\">Turtle</th>",
